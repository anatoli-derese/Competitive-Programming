t=int(input())                                                             
for i in range (t):
    n=int(input())
    arr=[int(i)for i in input().split()]
    arr.sort()
    cnt=0
    for i in range(1,n):
        if arr [i]-arr[i-1]>1:
           cnt=1
    if cnt==1:
         print("NO")  
    else:
        print("YES")
